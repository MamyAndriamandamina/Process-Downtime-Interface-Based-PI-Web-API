@{var x = ViewData["enumvaljson"] as List<EnumerationSets>;}

@if (User.Identity.IsAuthenticated)
{
    if (x != null) 
    { 
            @foreach (var item in x)
            {
                <p><strong>Crew: </strong>@item.Crew</p>
                <p><strong>Description:  </strong>@item.Description</p>
                <p><strong>Field: </strong>@item.Field</p>
                <p><strong>Schedule: </strong>@item.Schedule</p>
                <p><strong>Scope: </strong>@item.Scope</p>
            }
    }
}
